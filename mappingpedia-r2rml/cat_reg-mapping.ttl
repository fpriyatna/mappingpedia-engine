@base <http://mappingpedia.linkeddata.es/cat-reg/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix metashare: <http://purl.org/net/def/metashare#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix reteleshare: <https://w3id.org/def/retele-share#> .


<TriplesMapLexicalData> a rr:TriplesMap;

    rr:logicalTable [ rr:tableName "lexical_data" ];

    rr:subjectMap [
        rr:termType rr:IRI;
        rr:template "http://retele.linkeddata.es/resource/LexicalConceptualResource/{id}";
        rr:class metashare:LexicalConceptualResource;
    ];

    rr:predicateObjectMap [
        rr:predicate dct:title;
        rr:objectMap [ rr:termType rr:Literal; rr:column "resname" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:resourceShortName;
        rr:objectMap [ rr:termType rr:Literal; rr:column "shortnameres" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:LingualityType;
        rr:objectMap [ rr:termType rr:Literal; rr:column "lingtype" ]
    ];

    rr:predicateObjectMap [
        rr:predicate dct:language;
        rr:objectMap [ rr:termType rr:Literal; rr:column "languages" ]
    ];

    rr:predicateObjectMap [
        rr:predicate dct:description;
        rr:objectMap [ rr:termType rr:Literal; rr:column "description" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:url;
        rr:objectMap [ rr:termType rr:IRI; rr:column "url" ]
    ];

    rr:predicateObjectMap [
        rr:predicate reteleshare:islrn;
        rr:objectMap [ rr:termType rr:Literal; rr:column "islrn" ]
    ];

    rr:predicateObjectMap [
        rr:predicate dct:license;
        rr:objectMap [ rr:termType rr:Literal; rr:template "http://purl.org/NET/rdflicense/{licencia}" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:iprHolder;
        rr:objectMap [ rr:termType rr:Literal; rr:column "ipr" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:hasContactPerson;
        rr:objectMap [ rr:termType rr:IRI; rr:template "http://mappingpedia.linkeddata.es/resource/ContactPerson/{namecontact}{givennamecontact}" ]
    ];

    rr:predicateObjectMap [
        rr:predicate dct:creator;
        rr:objectMap [ rr:termType rr:Literal; rr:column "creador" ]
    ];

    rr:predicateObjectMap [
        rr:predicate dct:publisher;
        rr:objectMap [ rr:termType rr:Literal; rr:column "provrec" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:hasFunding;
        rr:objectMap [ rr:termType rr:IRI; rr:template "http://mappingpedia.linkeddata.es/resource/Funding/{funding}" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:creationStartDate;
        rr:objectMap [ rr:termType rr:Literal; rr:column "startdate" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:creationEndDate;
        rr:objectMap [ rr:termType rr:Literal; rr:column "enddate" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:lexicalConceptualResourceType;
        rr:objectMap [ rr:termType rr:Literal; rr:column "lextype" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:encodingLevel;
        rr:objectMap [ rr:termType rr:Literal; rr:column "codlevel" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:linguisticInformation;
        rr:objectMap [ rr:termType rr:Literal; rr:column "linginfo" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:Size;
        rr:objectMap [ rr:termType rr:Literal; rr:column "tamperlang" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:SizeUnit;
        rr:objectMap [ rr:termType rr:Literal; rr:column "sizeunit" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:languageVarietyType;
        rr:objectMap [ rr:termType rr:Literal; rr:column "varietytype" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:mimeType;
        rr:objectMap [ rr:termType rr:Literal; rr:column "mime" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:CharacterEncoding;
        rr:objectMap [ rr:termType rr:Literal; rr:column "charencoding" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:CharacterEncoding;
        rr:objectMap [ rr:termType rr:Literal; rr:column "charencoding" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:domain;
        rr:objectMap [ rr:termType rr:Literal; rr:column "dominio" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:toolDocumentationType;
        rr:objectMap [ rr:termType rr:Literal; rr:column "doctype" ]
    ];

.


<TriplesMapContactPerson> a rr:TriplesMap;

    rr:logicalTable [ rr:tableName "lexical_data" ];

    rr:subjectMap [
        rr:termType rr:IRI;
        rr:template "http://mappingpedia.linkeddata.es/resource/ContactPerson/{namecontact}{givennamecontact}";
        rr:class metashare:ContactPerson;
    ];

    rr:predicateObjectMap [
        rr:predicate foaf:name;
        rr:objectMap [ rr:termType rr:Literal; rr:column "namecontact" ]
    ];

    rr:predicateObjectMap [
        rr:predicate foaf:givenName;
        rr:objectMap [ rr:termType rr:Literal; rr:column "givennamecontact" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:email;
        rr:objectMap [ rr:termType rr:Literal; rr:column "emailcontact" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:url;
        rr:objectMap [ rr:termType rr:IRI; rr:column "urlcontacto" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:hasAffiliation;
        rr:objectMap [ rr:termType rr:IRI; rr:template "http://mappingpedia.linkeddata.es/resource/Organisation/{shortaffiliation}" ]
    ];

.

<TriplesMapOrganisation> a rr:TriplesMap;

    rr:logicalTable [ rr:tableName "lexical_data" ];

    rr:subjectMap [
        rr:termType rr:IRI;
        rr:template "http://mappingpedia.linkeddata.es/resource/Organisation/{shortaffiliation}";
        rr:class metashare:Organisation;
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:organizationShortName;
        rr:objectMap [ rr:termType rr:Literal; rr:column "shortaffiliation" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:departmentName;
        rr:objectMap [ rr:termType rr:Literal; rr:column "department" ]
    ];

    rr:predicateObjectMap [
        rr:predicate metashare:url;
        rr:objectMap [ rr:termType rr:Literal; rr:column "urlAffiliation" ]
    ];

.

<TriplesMapFunding> a rr:TriplesMap;

    rr:logicalTable [ rr:tableName "lexical_data" ];

    rr:subjectMap [
        rr:termType rr:IRI;
        rr:template "http://mappingpedia.linkeddata.es/resource/Funding/{funding}";
        rr:class metashare:Project;
    ];

    rr:predicateObjectMap [
        rr:predicate dct:title;
        rr:objectMap [ rr:termType rr:Literal; rr:column "funding" ]
    ];

.








