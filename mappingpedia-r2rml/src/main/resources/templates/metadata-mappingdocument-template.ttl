mpv:R2RMLMappingDocument rdfs:subClassOf dcterms:BibliographicResource .
mpimd:mappingDocument-$mappingDocumentID a mpv:R2RMLMappingDocument ;
    mpv:mappingDocumentIdentifier "mappingDocument-$mappingDocumentID" ;
    mpv:mappingDocumentTitle "$mappingDocumentTitle" ;
    mpv:mappingDocumentDateSubmitted "$mappingDocumentDateTimeSubmitted"^^xsd:dateTime;
    mpv:mappingDocumentCreator "$mappingDocumentCreator" ;
    mpv:mappingDocumentSubject "$mappingDocumentSubjects" ;
    mpv:hasDataset mpids:dataset-$mappingDocumentID ;
    #mpv:hasTriplesMap <TriplesMap1> #generated by mappingpedia-engine
    mpv:hasMappingDocumentFile "$mappingDocumentFilePath" ;
.
