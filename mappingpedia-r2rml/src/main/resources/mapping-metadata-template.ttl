@base <http://mappingpedia.linkeddata.es/instance#> .
@prefix dcterms: <http://purl.org/dc/elements/1.1/> .
@prefix mpv: <http://mappingpedia.linkeddata.es/vocabulary#> .
@prefix mpi: <http://mappingpedia.linkeddata.es/instance/> .
@prefix mpimd: <http://mappingpedia.linkeddata.es/instance/mappingdocument/> .
@prefix mpids: <http://mappingpedia.linkeddata.es/instance/dataset/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

mpv:R2RMLMappingDocument rdfs:subClassOf dcterms:BibliographicResource .
mpimd:mappingDocument-$mappingDocumentID a mpv:R2RMLMappingDocument ;
    mpv:mappingDocumentIdentifier "mappingDocument-$mappingDocumentID" ;
    mpv:mappingDocumentTitle "$mappingDocumentTitle" ;
    mpv:mappingDocumentDateSubmitted "$mappingDocumentDateTimeSubmitted"^^xsd:dateTime;
    mpv:mappingDocumentCreator "$mappingDocumentDCTCreator" ;
    mpv:mappingDocumentSubject "$mappingDocumentSubjects" ;
    mpv:hasDataset mpids:dataset-$mappingDocumentID ;
    #mpv:hasTriplesMap <TriplesMap1> #generated by mappingpedia-engine
    mpv:hasMappingDocumentFile "$mappingDocumentFilePath" ;
.

# mpv:Dataset rdfs:subClassOf dcat:Dataset .
mpi:dataset-$mappingDocumentID a mpv:Dataset ;
    mpv:datasetIdentifier "dataset-$mappingDocumentID" ;
    dcterms:title "$datasetTitle" ;
    dcat:keyword "$datasetKeywords" ;
.